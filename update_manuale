#!/bin/bash

source config.sh

test $LOGGING_ENABLE == true && echo "$(date '+%d/%m/%y %H:%m:%S') - Import Manuale" >> $LOGFIL

if [ -e "$FILE_manuale" ]; then
  egrep '^[^#]' $FILE_manuale > lists/manuale.new.tmp
  mv lists/manuale.new.tmp lists/manuale.new
elif [ ! -e lists/manuale.new ]; then
  : > lists/manuale.new
fi

test $LOGGING_ENABLE == true && echo "$(date '+%d/%m/%y %H:%m:%S') - Manuale Import Completed" >> $LOGFILE
